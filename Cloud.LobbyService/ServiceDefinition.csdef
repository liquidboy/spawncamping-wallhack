<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Cloud.LobbyService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WorkerRole name="Cloud.LobbyService.WorkerRole" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="Lobby.ServiceBusCredentials" />
      <Setting name="Lobby.StorageConnectionString" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="LobbyService" protocol="tcp" port="3000" />
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>